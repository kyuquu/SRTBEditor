{
    "dropdowns": {
        "tb-load": {
            "text": "LOAD",
            "options": [
                {
                    "text": "CREATE NEW",
                    "function": "loadTemplate('Custom.srtb')"
                },
                {
                    "text": "FROM FILE",
                    "function": "fileInput.click()"
                },
                {
                    "text": "FROM LINK",
                    "function": "loadFromLink()"
                }
            ]
        },
        "tb-save": {
            "text": "SAVE",
            "options": [
                {
                    "text": "SAVE AS .SRTB",
                    "function": "saveAsSRTB()"
                },
                {
                    "text": "SAVE AS .JSON",
                    "function": "saveAsJSON()"
                },
                {
                    "text": "SAVE AS .ZIP",
                    "function": "saveAsZIP()"
                }
            ]
        }
    },
    "buttons": {
        "jv-save": {
            "text": "SAVE CHANGES",
            "function": "saveEditorChanges()"
        },
        "jv-discard": {
            "text": "DISCARD CHANGES",
            "function": "discardEditorChanges()"
        },
        "dv-stack": {
            "text": "Stack nearby notes",
            "function": "stackNearbyNotesAllDiffs()"
        },
        "dv-import-lyrics": {
            "text": "Import lyrics",
            "function": "lyricInput.click()"
        },
        "dv-set-serialization": {
            "text": "Modernize note format",
            "function": "changeNoteEncodings(2)"
        },
        "popup": {
            "text": "create popup",
            "function": "popup('title', 'content', [{text: 'Cancel', action: 'closePopup()', type: 0}])"
        }
    },
    "text-inputs": {
        "bv-title": {
            "text": "Title",
            "function": "processBVInput('text', 'title', 'TrackInfo')"
        },
        "bv-subtitle": {
            "text": "Subtitle",
            "function": "processBVInput('text', 'subtitle', 'TrackInfo')"
        },
        "bv-artistName": {
            "text": "Artist",
            "function": "processBVInput('text', 'artistName', 'TrackInfo')"
        },
        "bv-featArtists": {
            "text": "Featured Artist",
            "function": "processBVInput('text', 'featArtists', 'TrackInfo')"
        },
        "bv-charter": {
            "text": "Charter",
            "function": "processBVInput('text', 'charter', 'TrackInfo')"
        }

    },
    "number-inputs": {
        "bv-difficultyRating0": {
            "text": "",
            "function": "processBVInput('number', 'difficultyRating', 'TrackData', '0')"
        },
        "bv-difficultyRating1": {
            "text": "",
            "function": "processBVInput('number', 'difficultyRating', 'TrackData', '1')"
        },
        "bv-difficultyRating2": {
            "text": "",
            "function": "processBVInput('number', 'difficultyRating', 'TrackData', '2')"
        },
        "bv-difficultyRating3": {
            "text": "",
            "function": "processBVInput('number', 'difficultyRating', 'TrackData', '3')"
        },
        "bv-difficultyRating4": {
            "text": "",
            "function": "processBVInput('number', 'difficultyRating', 'TrackData', '4')"
        },
        "bv-difficultyRating5": {
            "text": "",
            "function": "processBVInput('number', 'difficultyRating', 'TrackData', '5')"
        }
    },
    "checkboxes": {
        "bv-allowCustomLeaderboardCreation": {
            "text": "Allow Leaderboard Creation",
            "function": "processBVInput('checkbox', 'allowCustomLeaderboardCreation', 'TrackInfo')"
        },
        "bv-difficulty-active0": {
            "text": "Easy",
            "function": "toggleDifficultyActive(0)"
        },
        "bv-difficulty-active1": {
            "text": "Normal",
            "function": "toggleDifficultyActive(1)"
        },
        "bv-difficulty-active2": {
            "text": "Hard",
            "function": "toggleDifficultyActive(2)"
        },
        "bv-difficulty-active3": {
            "text": "Expert",
            "function": "toggleDifficultyActive(3)"
        },
        "bv-difficulty-active4": {
            "text": "XD",
            "function": "toggleDifficultyActive(4)"
        },
        "bv-difficulty-active5": {
            "text": "RemiXD",
            "function": "toggleDifficultyActive(5)"
        }
    },
    "file-inputs": {
        "bv-album-art": {
            "text": "Album Art",
            "accept": ".png, .jpg, .jpeg",
            "function": "updateAlbumArt()"
        },
        "bv-audio-clips": {
            "text": "Audio Clips",
            "accept": ".mp3, .ogg",
            "function": "updateAudioClips()"
        }
    },
    "WAWA select-inputs": {
        "WAWAbv-category": {
            "text": "Category",
            "options": [
                "Track Info",
                "Track Data",
                "Clip Info",
                "Dynamic Track Speed",
                "Speen Chroma"
            ],
            "default": 0,
            "function": "switchToCategory"
        },
        "WAWAbv-difficulty": {
            "text": "Difficulty",
            "options": [
                "Easy",
                "Normal",
                "Hard",
                "Expert",
                "XD",
                "RemiXD"
            ],
            "default": 2,
            "function": "switchToDifficulty"
        }
    }
}