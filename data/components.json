{
    "dropdowns": {
        "tb-load": {
            "text": "LOAD",
            "options": [
                {
                    "text": "CREATE NEW",
                    "function": "loadTemplate('Custom.srtb')"
                },
                {
                    "text": "FROM FILE",
                    "function": "fileInput.click()"
                },
                {
                    "text": "FROM LINK",
                    "function": "loadFromLink()"
                }
            ]
        },
        "tb-save": {
            "text": "SAVE",
            "options": [
                {
                    "text": "SAVE AS .SRTB",
                    "function": "saveAsSRTB()"
                },
                {
                    "text": "SAVE AS .JSON",
                    "function": "saveAsJSON()"
                },
                {
                    "text": "SAVE AS .ZIP",
                    "function": "saveAsZIP()"
                }
            ]
        }
    },
    "buttons": {
        "jv-save": {
            "text": "SAVE CHANGES",
            "function": "saveEditorChanges()"
        },
        "jv-discard": {
            "text": "DISCARD CHANGES",
            "function": "discardEditorChanges()"
        }
    },
    "text-inputs": {
        "bv-title": {
            "text": "Title",
            "function": "processBVInput('text', 'title', 'TrackInfo')"
        },
        "bv-subtitle": {
            "text": "Subtitle",
            "function": "processBVInput('text', 'subtitle', 'TrackInfo')"
        },
        "bv-artistName": {
            "text": "Artist",
            "function": "processBVInput('text', 'artistName', 'TrackInfo')"
        },
        "bv-featArtists": {
            "text": "Featured Artist",
            "function": "processBVInput('text', 'featArtists', 'TrackInfo')"
        },
        "bv-charter": {
            "text": "Charter",
            "function": "processBVInput('text', 'charter', 'TrackInfo')"
        },
        "dv-summary": {
            "text": "Summary",
            "function": ""
        }

    },
    "number-inputs": {
        "bv-difficultyRating-0": {
            "text": "Easy:",
            "function": "processBVInput('text', 'difficultyRating', 'TrackData', '0')"
        },
        "bv-difficultyRating-1": {
            "text": "Normal:",
            "function": "processBVInput('text', 'difficultyRating', 'TrackData', '1')"
        },
        "bv-difficultyRating-2": {
            "text": "Hard:",
            "function": "processBVInput('text', 'difficultyRating', 'TrackData', '2')"
        },
        "bv-difficultyRating-3": {
            "text": "Expert:",
            "function": "processBVInput('text', 'difficultyRating', 'TrackData', '3')"
        },
        "bv-difficultyRating-4": {
            "text": "XD:",
            "function": "processBVInput('text', 'difficultyRating', 'TrackData', '4')"
        },
        "bv-difficultyRating-5": {
            "text": "RemiXD:",
            "function": "processBVInput('text', 'difficultyRating', 'TrackData', '5')"
        }
    },
    "checkboxes": {
        "bv-allowCustomLeaderboardCreation": {
            "text": "Allow Leaderboard Creation",
            "function": "processBVInput('checkbox', 'allowCustomLeaderboardCreation')"
        }
    },
    "file-inputs": {
        "bv-album-art": {
            "text": "Album Art",
            "accept": ".png, .jpg, .jpeg",
            "function": "updateAlbumArt()"
        },
        "bv-audio-clips": {
            "text": "Audio Clips",
            "accept": ".mp3, .ogg",
            "function": "updateAudioClips()"
        }
    },
    "select-inputs": {
        "bv-category": {
            "text": "Category",
            "options": [
                "Track Info",
                "Track Data",
                "Clip Info",
                "Dynamic Track Speed",
                "Speen Chroma"
            ],
            "default": 0,
            "function": "switchToCategory"
        },
        "bv-difficulty": {
            "text": "Difficulty",
            "options": [
                "Easy",
                "Normal",
                "Hard",
                "Expert",
                "XD",
                "RemiXD"
            ],
            "default": 2,
            "function": "switchToDifficulty"
        }
    }
}